<?xml version="1.0" encoding="UTF-8"?> <kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:atom="http://www.w3.org/2005/Atom">
<Document>
	<Folder>
		<name>My Places</name>
		<open>1</open>
		<Style>
			<ListStyle>
				<listItemType>check</listItemType>
				<ItemIcon>
					<state>open</state>
					<href>:/mysavedplaces_open.png</href>
				</ItemIcon>
				<ItemIcon>
					<state>closed</state>
					<href>:/mysavedplaces_closed.png</href>
				</ItemIcon>
				<bgColor>00ffffff</bgColor>
				<maxSnippetLines>2</maxSnippetLines>
			</ListStyle>
		</Style>
		<Folder>
			<name>KML Sync</name>
			<NetworkLink>
				<name>Master KML</name>
				<Link>
					<href>##LG_KML_SYNC_BASE##/kml/master.kml</href>
				</Link>
			</NetworkLink>
			<NetworkLink>
				<name>KML Update</name>
				<Link>
					<href>##LG_KML_SYNC_BASE##/sync_nlc.php?tag=##EARTH_KML_SYNC_TAG##</href>
					<refreshMode>onInterval</refreshMode>
					<refreshInterval>1</refreshInterval>
				</Link>
			</NetworkLink>
			<NetworkLink>
				<name>View Placemark</name>
				<description>The view-based refresh allows the remote server to calculate
			    the center of your screen and return a placemark.</description>
				<Link>
					<href>##LG_KML_SYNC_BASE##/cgi-bin/viewBookmark.py</href>
					<refreshMode>onInterval</refreshMode>
					<refreshInterval>1</refreshInterval>
					<viewRefreshMode>onStop</viewRefreshMode>
					<viewRefreshTime>1</viewRefreshTime>
					<viewFormat>BBOX=[bboxWest],[bboxSouth],[bboxEast],[bboxNorth];LOOKAT=[lookatLon],[lookatLat],[lookatRange],[lookatTilt],[lookatHeading];CAMERA=[cameraLon],[cameraLat],[cameraAlt],[lookatRange],[lookatTilt],[lookatHeading]</viewFormat>
				</Link>
			</NetworkLink>
		</Folder>
		<Folder>
			<name>Content</name>
			<NetworkLink>
				<name>Static KML</name>
				<Link>
					<href>##LG_STATIC_KML##</href>
				</Link>
			</NetworkLink>
		</Folder>
	</Folder>
</Document>
</kml>
